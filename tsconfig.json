		"allowJs": true,
		"checkJs": true,
		"noEmit": true,
		"noImplicitAny": true,
		"target": "es2022",
		"module": "node16",
		"moduleResolution": "node16",
		"skipLibCheck": true,
		"allowSyntheticDefaultImports": true,
		"baseUrl": ".",
	},
	"include": ["**/*.js", "placeholders.d.ts"]
}